{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "tests",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib"
        ]
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "example: tree",
      "cargo": {
        "args": [
          "build",
          "--example=tree",
          "--features=fs"
        ],
        "filter": {
          "name": "tree",
          "kind": "example"
        }
      },
      "args": ["."],
      "cwd": "${workspaceFolder}"
    }
  ]
}
